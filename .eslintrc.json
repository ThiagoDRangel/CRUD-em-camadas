{
  "env": {
    "es2020": true
  },
  "extends": "trybe-backend",
  "rules": {
    "sonarjs/no-duplicate-string": [
      "error",
      5
    ],
    "indent": "error"
  },
  "overrides": [
    {
      "files": [
        "*.test.js"
      ],
      "rules": {
        "max-lines-per-function": 0,
        "sonarjs/no-identical-functions": 0
      }
    }
  ]
}